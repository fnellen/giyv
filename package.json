{
  "name": "givy-landingpage",
  "type": "module",
  "version": "1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && pagefind --site dist && cp -r dist/pagefind public/",
    "preview": "astro preview",
    "astro": "astro",
    "i18n:extract": "astro-i18n extract",
    "i18n:generate:pages": "astro-i18n generate:pages --purge",
    "i18n:generate:types": "astro-i18n generate:types",
    "i18n:sync": "npm run i18n:generate:pages && npm run i18n:generate:types"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "astro": "^3.0.7",
    "astro-compressor": "^0.4.1",
    "astro-i18n": "^2.1.18",
    "astro-icon": "^0.8.1",
    "astro-navbar": "^2.3.0",
    "astro-seo": "^0.8.0",
    "markdown-it": "^13.0.2",
    "marked": "^10.0.0",
    "pagefind": "^1.0.4",
    "qs": "^6.11.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.11.0",
    "strip-markdown": "^6.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/markdown-it": "^13.0.7",
    "@types/qs": "^6.9.11",
    "@types/sanitize-html": "^2.9.5",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2"
  }
}
